 eid = {356.03},
 month = {January},
 pages = {356.03},
 series = {American Astronomical Society Meeting Abstracts},
 title = {Examining High Redshift Rotation Curves and Dark Matter Profiles Outside the Local Universe},
 volume = {231},
 year = {2018}
}
